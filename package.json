{
  "name": "bequest",
  "version": "1.4.1",
  "description": "An ajax class",
  "main": "dist/ajax.js",
  "module": "index.js",
  "scripts": {
    "start": "scriptkit",
    "build": "scriptkit build",
    "dev": "scriptkit dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "scriptkit"
  },
  "watch": {
    "lib/ajax.js": [
      "lint",
      "build"
    ]
  },
  "keywords": [
    "browser",
    "ajax",
    "request"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firstandthird/bequest.git"
  },
  "bugs": {
    "url": "https://github.com/firstandthird/bequest/issues"
  },
  "homepage": "https://github.com/firstandthird/bequest#readme",
  "devDependencies": {
    "eslint-config-firstandthird": "3.2.0",
    "eslint-plugin-import": "2.7.0",
    "scriptkit": "0.2.0"
  },
  "scriptkit": {
    "test": {
      "enabled": false
    },
    "files": {
      "dist/ajax.js": "index.js"
    },
    "scripts": {
      "babel": {
        "exclude": [
          "node_modules/tape-rollup/*"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "firstandthird",
    "env": {
      "browser": true
    }
  }
}
