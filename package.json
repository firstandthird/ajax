{
  "name": "bequest",
  "version": "2.0.0",
  "description": "An ajax class",
  "main": "index.js",
  "module": "dist/ajax.esm.js",
  "scripts": {
    "start": "scriptkit",
    "build": "scriptkit build",
    "dev": "scriptkit dev",
    "test": "cat test/bequest.test.dist.js | tape-run --static . --browser phantomjs --render tap-spec",
    "prepublishOnly": "scriptkit"
  },
  "watch": {
    "lib/ajax.js": [
      "lint",
      "build"
    ]
  },
  "keywords": [
    "browser",
    "ajax",
    "request"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firstandthird/bequest.git"
  },
  "bugs": {
    "url": "https://github.com/firstandthird/bequest/issues"
  },
  "homepage": "https://github.com/firstandthird/bequest#readme",
  "devDependencies": {
    "eslint-config-firstandthird": "^4.3.0",
    "eslint-plugin-import": "^2.16.0",
    "phantomjs-prebuilt": "^2.1.16",
    "scriptkit": "^2.0.1",
    "tap-spec": "^5.0.0",
    "tape-rollup": "^4.6.4",
    "tape-run": "^5.0.0"
  },
  "scriptkit": {
    "files": {
      "dist/ajax.js": "index.js",
      "test/bequest.test.dist.js": "test/bequest.test.js"
    },
    "scripts": {
      "babel": {
        "exclude": [
          "node_modules/tape-rollup/*"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "firstandthird",
    "env": {
      "browser": true
    }
  }
}
